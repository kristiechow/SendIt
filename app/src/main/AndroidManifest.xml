<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kristie.sendit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service android:name=".AlarmServiceEmail">

        </service>
        <activity
            android:name="com.example.kristie.sendit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.kristie.sendit.LoginActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.kristie.sendit.SignupActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.kristie.sendit.SMSActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.kristie.sendit.MailSenderActivity" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.kristie.sendit.SuccessEmailActivity" android:theme="@style/AppTheme.Dark"/>
    </application>

</manifest>